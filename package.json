{
  "engines": {
    "node": "16.x"
  },
  "name": "menuworker",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run prod",
    "prod": "npm-run-all -p prod:*",
    "prod:server": "NODE_ENV=production node --experimental-specifier-resolution=node ./src/index.js",
    "prod:queue": "NODE_ENV=production node --experimental-specifier-resolution=node ./src/queue.js",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "nodemon --experimental-specifier-resolution=node ./src/index.js",
    "dev:queue": "node --experimental-specifier-resolution=node src/queue.js",
    "test": "NODE_ENV=test npx mocha --recursive",
    "pretest": "NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:create:test": "NODE_ENV=test npx sequelize-cli db:create",
    "no-reset:test": "NODE_ENV=test npx mocha --recursive",
    "no-reset:test:single": "NODE_ENV=test npx mocha"
  },
  "author": "",
  "license": "ISC",
  "mocha": {
    "node-option": [
      "experimental-specifier-resolution=node"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.2",
    "bcrypt": "^5.1.0",
    "bootstrap": "^5.2.3",
    "boxicons": "^2.1.4",
    "bull": "^4.10.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "eslint-config-google": "^0.14.0",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "html-pdf-node": "^1.0.8",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "jquery": "^2.2.4",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.26.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "sequelize-cli": "^6.6.0"
  }
}
